{\s17\sa60\sb30\widctlpar\qj \fs22\cgrid 
This demonstrates a simple way to get an instance of {\b Thor::Peripheral::UART::UARTClass} up and running quickly.\par
{
\par
\pard\plain \s40\li0\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid #include <Thor/include/definitions.h>\par
#include <Thor/include/uart.h>\par
\par
using namespace Thor::Definitions::Serial;\par
using namespace Thor::Peripheral::UART;\par
\par
int main(void)\par
\{\par
    /* Some test messages to send out */\par
    const char* test1 = "Hey mate.\\r\\n";\par
    const char* test2 = "Testing testing!\\r\\n";\par
    const char* test3 = "I love watermelon.\\r\\n";\par
\par
    /* Create a brand new uart instance from UART4 */\par
    UARTClass_sPtr uart = UARTClass::create(4);\par
    uart->begin(SERIAL_BAUD_115200);\par
\par
    /* Comment/Uncomment to switch modes */\par
    uart->setBlockMode(UARTPeriph::TX);\par
    //uart->setITMode(UARTPeriph::TX);\par
    //uart->setDMAMode(UARTPeriph::TX);\par
\par
    /* If in IT or DMA mode, the text will be buffered */\par
    uart->write(test1);\par
    uart->write(test2);\par
    uart->write(test3);\par
\par
    return 0;\par
\}\par
\par}
